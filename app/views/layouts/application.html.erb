<!DOCTYPE html>
<html>
  <head>
    <title>AdminPanel3</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

  
    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
   
   <nav class="navbar navbar-light bg-light">
      <a class="navbar-brand" href="#">Navbar</a>
    <% if current_user %>
      <li><a href="<%= users_path %>" id="welcome-nav">Welcome, <%= current_user.email%></a></li>
      <li><a href="<%= students_path %>">Students</a></li>
      <li><a href="<%= instructors_path %>">Instructors</a></li>
      <li><a href="<%= courses_path %>">Courses</a></li>
      <li><a href="<%= cohorts_path %>">Cohorts</a></li>
      <li><a href="<%= students_cohorts_path %>">Student Cohort</a></li>
      <li> <%= link_to "Logout", logout_path, method: :delete %> </li>
    <% else %>
      <li> <%= link_to "Login", login_path %> </li>
      <li> <%= link_to "Signup", signup_path %> </li>
    <% end %>
    
      
  </nav>
  <% if flash[:notice]%>
  <%= flash[:notice]%>
<% end %>
<% if flash[:alert]%>
  <%= flash[:alert]%>
<% end %>
<br>

    <%= yield %>
  </body>
</html>
