
    <%= form_with model: @instructor do |f| %>
    
      <%= f.label :first_name %>
      <%= f.text_field :first_name, placeholder: "Rich" %>
      <%= f.label :last_name %>
      <%= f.text_field :last_name, placeholder: "Trapanese" %>
      <%= f.label :age %>
      <%= f.text_field :age, placeholder: "15" %>
      <%= f.label :salary %>
      <%= f.text_field :salary, placeholder: "10000000" %>
      <%= f.label :education %>
      <%= f.select(:education, options_for_select([['High School', "high school"], ['College',"college"], ['Masters',"master"], ['PHD',"PHD"]]), { include_blank: true }) %>
      <%= f.label :cohort_id %>
      <%= f.select :cohort_id, options_for_select(@cohorts, @instructor.id)%>
      <%= f.submit "Add Instructor"%>
  <% end %>
